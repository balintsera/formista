{
    "name": "evista/formista",
    "description": "A light form API",
    "keywords": [
        "form",
        "formista"
    ],
    "homepage": "https://github.com/serabalint/formista",
    "license": "MIT",
    "authors": [
        {
            "name": "Balint Sera",
            "email": "sera.balint@e-vista.hu",
            "homepage": "http://evista-agency.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Evista\\Formista\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evista\\Formista\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
